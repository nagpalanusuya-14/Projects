    {
      "cell_type": "code",
      "source": [
        "import re\n",
        "from datetime import datetime, timedelta\n",
        "def parse_date(date_str):\n",
        "    match = re.search(r'(\\d+)d', date_str)\n",
        "    if match:\n",
        "        days_ago = int(match.group(1))\n",
        "        return datetime.now() - timedelta(days=days_ago)\n",
        "    return None\n",
        "# Assuming 'df' has the 'Date' column you want to parse\n",
        "# Create a new column 'Date Posted' in df_cleaned based on the 'Date' column in df\n",
        "df_cleaned['Date Posted'] = df['Date'].apply(parse_date)\n",
        "df.head()\n"
      ],
